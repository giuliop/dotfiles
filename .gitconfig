[user]
    name = giulio
    email = giuliop@gmail.com
[core]
    editor = vim
    excludesfile = ~/dev/dotfiles/.gitignore_global
[github]
    user = giuliop
[alias]
    aa = add --all
    st = status
    last = log -1 HEAD
    tg = tag -l -n1
    ca = commit -a -m
    cm = commit -m
    dc = diff --cached
[push]
    ;git push will only push current branch when tracking enabled
    default = tracking
